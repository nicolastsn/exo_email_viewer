<h1>My Emails</h1>




<div class="container">
	<div class="object">
		<h4>
			<% @emails.each do |email| %>	
				<li>
					<%= email.object %>
				</li>
			<% end %>
		</h4>
	</div>

	<div class="body">
		<h4>
			<% @emails.each do |email| %>	
				<li>
					<%= email.body %>
				</li>
			<% end %>
		</h4>
	</div>
</div>